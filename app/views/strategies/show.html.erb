<!-- Specific strategy page -->

<p>  All Strategies  </p>

<table>
<tr>
<th>Title</th>
<th>body</th>
<th>Technology</th>
<th>Source</th>
</tr>

<% @strategies.each do |strategy| %>
<tr>
<td> <%= strategy.title %> </td>
<td> <%= strategy.body %> </td>
<td> <%= strategy.tech %> </td>
<td> <%= strategy.source %> </td>
<td> <%= link_to "Update", controller: "strategies", action: "update", id: strategy.id %> </td>
<td> <%= link_to "Destroy", strategy_path(strategy), method: "delete", id: strategy.id,  data: { confirm: 'Are you sure?' } %> </td>
</tr>
<% end %>
</table>
